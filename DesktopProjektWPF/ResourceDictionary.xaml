<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:DesktopProjektWPF.ViewModel"
                    xmlns:view="clr-namespace:DesktopProjektWPF.View">
    <DataTemplate DataType="{x:Type vm:FirstFormViewModel}">
        <view:FirstForm />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SecondFormViewModel}">
        <view:SecondForm />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:KierunkiListViewModel}">
        <view:KierunkiList />
    </DataTemplate>
</ResourceDictionary>